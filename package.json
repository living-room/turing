{
  "name": "@living-room/client-js",
  "version": "0.4.0",
  "description": "node.js and browser client for living room server",
  "main": "build/room.js",
  "browser": "build/room.browser.js",
  "bin": {
    "room": "examples/commandline.js"
  },
  "repository": "https://github.com/living-room/client-js",
  "author": "Jonathan Dahan",
  "license": "MIT",
  "devDependencies": {
    "chance": "^1.0.13",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.57.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "serve": "^6.5.5"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "service": "node -e 'require(\"@living-room/service-js/server.js\")'",
    "examples": "serve --cors examples",
    "format": "prettier-standard 'src/*.js' 'examples/*/*.js'",
    "start": "npm-run-all -p watch service examples",
    "subscribe": "node examples/commandline.js subscribe",
    "room": "node examples/commandline.js",
    "room:assert": "node examples/commandline.js assert",
    "room:retract": "node examples/commandline.js retract",
    "room:select": "node examples/commandline.js select",
    "systemd": "npm-run-all systemd:remove systemd:add",
    "systemd:add": "NODE=`which node` sudo -E bash -c '$NODE service.js add'",
    "systemd:remove": "NODE=`which node` sudo -E bash -c '$NODE service.js remove'"
  },
  "dependencies": {
    "@living-room/service-js": "^0.4.0",
    "iohook": "^0.1.15",
    "node-fetch": "^2.1.2",
    "prettier-standard": "^8.0.0",
    "socket.io-client": "^2.0.4"
  }
}
